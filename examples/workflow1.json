[
    {
        "setupVars": {
          "assign": [
            {
                "template_var1": 1
            },
            {
                "template_var2": "testing 123"
            }
          ]
        }
    },
    {
        "generateFile": {
            "call": "template.render",
            "args": {
                "template_url": "https://raw.githubusercontent.com/rizki96/workflow_cmd/master/examples/templates/temp1.tmpl",
                "output_path": "output/templates/temp1.out"
            },
            "result": "rendered_content"
        }
    },
    {
        "writeFile": {
            "call": "file.write",
            "args": {
                "input_string": "${rendered_content}",
                "output_path": "output/templates/write1.out"
            }
        }
    }
]